{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "production": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "preview": {
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "development": {
      "channel": "development",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
